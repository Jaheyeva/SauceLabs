{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://secure-test.ifbyphone.com/login.php"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": ".masked_input_box"
      },
      "text": "1234567890"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "PIN"
      },
      "text": "password"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "existingaccount"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Basic Services"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "/html/body/div/div[2]/ul/li[2]/ul/li[5]/a"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Create a new Virtual Receptionist"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "menu"
      },
      "text": "VR Automated Test"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "prompt"
      },
      "text": "This is an virtual receptionist automated test. All settings left default."
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "saveinfo"
      }
    }
  ]
}