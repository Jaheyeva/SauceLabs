{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://secure-test.ifbyphone.com/login.php"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": ".masked_input_box"
      },
      "text": "1234567890"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "PIN"
      },
      "text": "password"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "existingaccount"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Click-to-Call"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Create a new Click-to-Call"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='click_edit']/table/tbody/tr[1]/td[2]/input"
      },
      "text": "CTC Automated Test"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='click_edit']/table/tbody/tr[6]/td[2]/select/option[5]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "saveinfo"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='div_content_template']/table/tbody/tr[4]/td/input"
      }
    }
  ]
}