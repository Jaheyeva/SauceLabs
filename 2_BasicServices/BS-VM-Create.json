{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://secure-test.ifbyphone.com/login.php"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": ".masked_input_box"
      },
      "text": "1234567890"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "PIN"
      },
      "text": "password"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "existingaccount"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Basic Services"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Voice Mail"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Create a new Voice Mail Box"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "mailbox_identifier"
      },
      "text": "VM Automated Test"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "email_address"
      },
      "text": "jaheyeva@ifbyphone.com"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='div_main_wide']/div[5]"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "id",
        "value": "vmail_send_email"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='vmailform1']/ul[2]/li[3]/input"
      }
    }
  ]
}