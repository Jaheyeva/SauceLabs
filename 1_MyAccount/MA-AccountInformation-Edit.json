{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://secure-test.ifbyphone.com/login.php"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": ".masked_input_box"
      },
      "text": "7734490000"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "PIN"
      },
      "text": "password"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "existingaccount"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Account Information"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "first_name"
      },
      "text": "Julia"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "last_name"
      },
      "text": "Ah"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='account_form']//button[.='Save']"
      }
    }
  ]
}